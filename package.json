        "prettier": "^2.8.2",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "axios": "^1.2.3",
        "body-parser": "^1.20.1",
